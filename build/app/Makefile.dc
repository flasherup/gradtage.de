update_weatherbitsvc:
	sudo -S systemctl stop weatherbitsvc
	sudo -S sudo cp weatherbitsvc /usr/local/bin/
	sudo -S systemctl start weatherbitsvc

update_weatherbitupdatesvc:
	sudo -S systemctl stop weatherbitupdatesvc
	sudo -S sudo cp weatherbitupdatesvc /usr/local/bin/
	sudo -S systemctl start weatherbitupdatesvc

update_daydegreesvc:
	sudo -S systemctl stop daydegreesvc
	sudo -S sudo cp daydegreesvc /usr/local/bin/
	sudo -S systemctl start daydegreesvc

